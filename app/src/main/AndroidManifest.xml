<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kenilt.loopingviewpager.example">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".showHideFragmentExample.ShowHideFragmentActivity"></activity>
        <activity android:name=".indicatorExample.IndicatorExampleActivity" />
        <activity android:name=".fragmentStatePager.FragmentStatePagerExampleActivity" />
        <activity android:name=".fragmentPager.FragmentPagerExampleActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.psoffritti.librarysampleapptemplate.core.SampleAppTemplateActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".simpleExample.SimpleExampleActivity" />
    </application>

</manifest>
